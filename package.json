{
  "name": "s2hand-express-ts",
  "version": "1.0.0",
  "description": "",
  "main": "./build/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rm -rf ./build/",
    "build": "tsc",
    "start:dev": "cross-env NODE_ENV=development npm run start",
    "start:prod": "cross-env NODE_ENV=production npm run start",
    "prestart": "npm run build",
    "start": "node -r module-alias/register ./build/app.js",
    "serve:dev": "cross-env NODE_ENV=development nodemon",
    "serve:prod": "cross-env NODE_ENV=production nodemon",
    "compose:prod": "sh ./scripts/compose.sh",
    "compose:prod:down": "sh ./scripts/compose.sh down",
    "compose:dev": "sh ./scripts/compose.sh up dev",
    "compose:dev:down": "sh ./scripts/compose.sh down dev",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "eslint --ext .ts,.tsx src/ --fix"
  },
  "author": "tuan.daothien",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "auto-bind": "^4.0.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.4",
    "typeorm": "^0.2.34",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "_moduleAliases": {
    "@": "build"
  }
}
